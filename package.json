  "devDependencies": {
    "@types/node": "^25.0.3",
    "discord-api-types": "^0.38.37",
    "eslint": "^9.39.2",
    "nodemon": "^3.1.9",
    "tsx": "^4.21.0",
    "typescript": "^5.9.3"
  }
